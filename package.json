{
  "name": "svelt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "nodemon -e *.svelte --exec 'npm run format:files'",
    "format:files": "prettier --write --plugin-search-dir=. ./**/**/*.svelte",
    "dev": "nodemon -e js,ts,json server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
    "preview": "cross-env NODE_ENV=production node server"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "bootstrap-icons": "^1.10.5",
    "cloudinary": "^1.37.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "flowbite": "^1.7.0",
    "flowbite-svelte": "^0.39.2",
    "jsonwebtoken": "^9.0.1",
    "luxon": "^3.3.0",
    "mongoose": "^7.3.2",
    "nodemailer": "^6.9.3",
    "otp-generators": "^1.0.3",
    "returner-pro-dep": "^0.0.2",
    "sirv": "^2.0.3",
    "socket.io": "^4.7.1",
    "tailwind-merge": "^1.13.2"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.25",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.5",
    "svelte-routing": "^1.11.0",
    "tailwindcss": "^3.3.2",
    "vite": "^4.4.2"
  }
}
